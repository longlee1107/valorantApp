{
  "cli": {
    "version": ">= 1.1.1"
  },
  "build": {
    "production": {
      "node": "16.13.0",
      "env": {
        "API_URL": "https://valorant-api.com/v1/"
      }
    },
    "preview": {
      "extends": "production",
      "distribution": "internal",
      "env": {
        "API_URL": "https://valorant-api.com/v1/"
      }
    },
    "development": {
      "extends": "production",
      "developmentClient": "true",
      "distribution": "internal",
      "env": {
        "API_URL": "https://valorant-api.com/v1/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
